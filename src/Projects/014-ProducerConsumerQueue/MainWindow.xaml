<Window x:Class="ProducerConsumerEventWaitHandle.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProducerConsumerEventWaitHandle"
        mc:Ignorable="d"
        Title="MainWindow" Height="250" Width="800">
    <StackPanel Orientation="Vertical">
        <Label x:Name="m_labelValues" Height="30"/>
        <Button x:Name="m_buttonStart" Content="Start" Click="m_buttonStart_Click"/>
    </StackPanel>
</Window>
